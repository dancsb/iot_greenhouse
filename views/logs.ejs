<%- include('header') -%>
<%- include('navbar') -%>
<div class="container mt-3">
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <table class="table table-sm table-dark table-borderless">
                <tbody>
                    <% logs.forEach(function(sensorboxLogs){ %>
                        <tr>
                            <td><%= sensorboxLogs.serialNumber %></td>
                        </tr>
                        <% sensorboxLogs.recentLogs.forEach((l) => { %>
                            <tr>
                                <td style="width: 150px;">[<%= l.date.getFullYear() %>.<%= (l.date.getMonth()+1 > 9) ? l.date.getMonth()+1 : "0" + (l.date.getMonth()+1) %>.<%= (l.date.getDate() > 9) ? l.date.getDate() : "0" + l.date.getDate() %>. <%= (l.date.getHours() > 9) ? l.date.getHours() : "0" + l.date.getHours() %>:<%= (l.date.getMinutes() > 9) ? l.date.getMinutes() : "0" + l.date.getMinutes() %>]</td>
                                <td style="color: red;"><%= l.action %></td>
                            </tr>
                        <% }); %>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>
<%- include('footer') -%>